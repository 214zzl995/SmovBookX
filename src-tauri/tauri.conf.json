{
  "package": {
    "productName": "SmovBook",
    "version": "0.1.9"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.leri.smovbook",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Copyright (c) 2021 Leri",
      "category": "DeveloperTool",
      "shortDescription": "smovbook app",
      "longDescription": "",
      "deb": {
        "depends": [],
        "files": {}
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix":{
          "language": ["zh-CN"]
        }
      }
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://dwdev-generic.pkg.coding.net/smovbook/smovbookx/updater.json?version=latest"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJCNEVBNkZBODc1REVFMzEKUldReDdsMkgrcVpPSzY5aGQ2anFoQ2NLWGhmR28xeDRZNDJ2LzcxNW1YT0t5L29hNXhmdE5WYU0K"
    },
    "allowlist": {
      "all": true,
      "protocol": {
        "asset": true,
        "assetScope": ["*"]
      },
      "shell":{
        "all":true,
        "open":".*"
      }
    },
    "windows": [
      {
        "title": "SMOVBOOK",
        "minWidth": 950,
        "minHeight": 650,
        "alwaysOnTop": false,
        "decorations": false,
        "resizable": true,
        "fullscreen": false,
        "transparent": false,
        "focus":true,
        "fileDropEnabled":false
      }
    ],
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: file: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self' asset: https://asset.localhost" 
    }
  }
}
